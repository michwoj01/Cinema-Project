<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.ii.cinemaProject.controller.LoginPageController">
    <Label text="Login Page">
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin></Label>
    <Label text="Email">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></Label>
    <TextField fx:id="email" alignment="CENTER">
      <VBox.margin>
         <Insets bottom="5.0" left="20.0" right="20.0" />
      </VBox.margin></TextField>
    <Button onAction="#login" text="Login" />

</VBox>
